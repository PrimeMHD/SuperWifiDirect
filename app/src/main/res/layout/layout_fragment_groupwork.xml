<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="hiveconnect.com.superwifidirect.Activity.MainActivity">

    <Button
        android:id="@+id/button_GroupMaster"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GroupMaster"
        android:layout_marginTop="30dp"
        android:layout_centerHorizontal="true"
        />
    <Button
        android:id="@+id/button_GroupSlave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/button_GroupMaster"
        android:layout_marginTop="200dp"
        android:text="GroupSlave"
        />


</RelativeLayout>
